    {
      "parameters": {
        "jsCode": "// Determine which push this is and create commit message\nconst triggerName = $input.item.json.name || '';\nlet pushNumber = 1;\nlet pushTime = '10:30 AM';\n\nif (triggerName.includes('1030') || triggerName.includes('10:30')) {\n  pushNumber = 1;\n  pushTime = '10:30 AM';\n} else if (triggerName.includes('1pm') || triggerName.includes('1:00 PM')) {\n  pushNumber = 2;\n  pushTime = '1:00 PM';\n} else if (triggerName.includes('5pm') || triggerName.includes('5:00 PM')) {\n  pushNumber = 3;\n  pushTime = '5:00 PM';\n}\n\nreturn {\n  json: {\n    pushNumber: pushNumber,\n    pushTime: pushTime,\n    commitMessage: `Automated build - Push ${pushNumber} at ${pushTime}`,\n    timestamp: new Date().toISOString()\n  }\n};"
      },
      "id": "prepare-push",
      "name": "Prepare Push Data",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [450, 350]
    },
