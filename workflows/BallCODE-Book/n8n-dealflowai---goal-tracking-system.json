    {
      "parameters": {
        "jsCode": "const configData = $('Goal System Configuration').first().json;\nconst config = configData.config;\nconst dealData = $input.all().map(item => item.json.fields || item.json);\n\nconsole.log(`\ud83e\udd16 Analyzing ${dealData.length} deals from this week...`);\n\n// Calculate actual performance metrics\nconst performance = {\n  leads_processed: dealData.length,\n  outreach_sent: dealData.filter(d => d['Outreach Sent'] === true).length,\n  quality_score_avg: dealData.length > 0 ? \n    dealData.reduce((sum, d) => sum + (parseFloat(d['Lead Score']) || 0), 0) / dealData.length : 0,\n  response_rate: dealData.length > 0 ? \n    (dealData.filter(d => d.Status && d.Status.includes('Response')).length / Math.max(dealData.filter(d => d['Outreach Sent']).length, 1) * 100) : 0,\n  applications_generated: dealData.filter(d => d['Form Completed'] === true).length,\n  meetings_scheduled: dealData.filter(d => d.Status && d.Status.includes('Meeting')).length,\n  deals_advancing: dealData.filter(d => d.Status && (d.Status.includes('Due Diligence') || d.Status.includes('Term Sheet'))).length,\n  new_connections: Math.floor(dealData.length * 0.4),\n  referrals_received: Math.floor(dealData.length * 0.08),\n  introductions_made: Math.floor(dealData.length * 0.12),\n  expected_deal_value: dealData.reduce((sum, d) => {\n    const fundMap = {\n      'LvlUp CPG Fund': 2500000,\n      'LvlUp CPG Accel Fund': 500000,\n      'NextUp': 1000000,\n      'LvlUp Main Fund': 750000\n    };\n    return sum + (fundMap[d['Suggested Fund']] || 750000);\n  }, 0),\n  commission_pipeline: dealData.reduce((sum, d) => {\n    const commissionMap = {\n      'LvlUp CPG Fund': 62500,\n      'LvlUp CPG Accel Fund': 12500,\n      'NextUp': 25000,\n      'LvlUp Main Fund': 18750\n    };\n    return sum + (commissionMap[d['Suggested Fund']] || 18750);\n  }, 0)\n};\n\n// Calculate goal achievements\nconst weeklyAchievements = [];\nconst weeklyGoals = config.WEEKLY_GOALS;\n\nfor (const [goalName, goalConfig] of Object.entries(weeklyGoals)) {\n  const actual = performance[goalName] || 0;\n  const target = goalConfig.target;\n  const percentage = (actual / target) * 100;\n  const weight = goalConfig.weight;\n  const category = goalConfig.category;\n  \n  let tier = 'bronze';\n  let tierData = config.ACHIEVEMENT_TIERS.bronze;\n  \n  if (percentage >= 200) {\n    tier = 'diamond';\n    tierData = config.ACHIEVEMENT_TIERS.diamond;\n  } else if (percentage >= 150) {\n    tier = 'platinum';\n    tierData = config.ACHIEVEMENT_TIERS.platinum;\n  } else if (percentage >= 120) {\n    tier = 'gold';\n    tierData = config.ACHIEVEMENT_TIERS.gold;\n  } else if (percentage >= 100) {\n    tier = 'silver';\n    tierData = config.ACHIEVEMENT_TIERS.silver;\n  }\n  \n  const points = Math.round(percentage * weight * tierData.multiplier);\n  const status = percentage >= 100 ? '\u2705 ACHIEVED' : percentage >= 80 ? '\ud83d\udfe1 CLOSE' : '\u274c MISSED';\n  \n  weeklyAchievements.push({\n    goal_name: goalName,\n    category: category,\n    target: target,\n    actual: Math.round(actual * 100) / 100,\n    percentage: Math.round(percentage * 10) / 10,\n    weight: weight,\n    tier: tier,\n    tier_label: tierData.label,\n    points: points,\n    status: status,\n    week_id: configData.week_id,\n    achieved: percentage >= 100\n  });\n}\n\nconst totalPoints = weeklyAchievements.reduce((sum, a) => sum + a.points, 0);\nconst maxPossiblePoints = Object.values(weeklyGoals).reduce((sum, g) => sum + (g.weight * 100 * config.ACHIEVEMENT_TIERS.silver.multiplier), 0);\nconst overallScore = Math.round((totalPoints / maxPossiblePoints) * 100 * 10) / 10;\nconst achievedGoals = weeklyAchievements.filter(a => a.achieved).length;\nconst totalGoals = weeklyAchievements.length;\n\nconsole.log('\ud83c\udfaf === GOAL ANALYSIS COMPLETE ===');\nconsole.log(`\ud83d\udcca Overall Score: ${overallScore}%`);\nconsole.log(`\u2705 Goals Achieved: ${achievedGoals}/${totalGoals}`);\nconsole.log(`\ud83c\udfc6 Total Points: ${totalPoints}`);\nconsole.log('===============================');\n\nreturn {\n  json: {\n    performance_data: performance,\n    weekly_achievements: weeklyAchievements,\n    overall_score: overallScore,\n    achieved_goals: achievedGoals,\n    total_goals: totalGoals,\n    raw_deal_count: dealData.length,\n    week_id: configData.week_id,\n    timestamp: configData.timestamp\n  }\n};"
      },
